{
	"name": "Currl",
	"short_name": "Currl",
	"description": "Save, tag & discover links you love from around the web",
	"icons": [
		{
			"src": "icon.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "any"
		},
		{
			"src": "icon-maskable.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "maskable"
		},
		{
			"src": "favicon.svg",
			"sizes": "any"
		}
	],
	"theme_color": "#fff",
	"background_color": "#fff",
	"display": "standalone",
	"start_url": "/",
	"permissions": ["notifications"],
	"share_target": {
		"action": "/bookmark",
		"method": "GET",
		"params": {
			"url": "link"
		}
	}
}
